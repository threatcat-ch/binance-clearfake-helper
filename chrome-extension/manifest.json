{
  "manifest_version": 3,
  "name": "Binance ClickFix Toolset",
  "version": "1.0",
  "description": "An extension to help keep an overview of ClickFix contracts.",
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["scripting", "activeTab"],
  "web_accessible_resources": [
    {
      "resources": ["pako.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
